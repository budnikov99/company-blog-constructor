{% for module in data.blocks.footer_social.modules %}
<a class="footer-social-button" href="{{module.data.href}}">
    {% if module.data.text|lower == 'twitter' %}
    {{twitter}}
    {% elseif module.data.text|lower == 'facebook' %}
    {{facebook}}
    {% elseif module.data.text|lower == 'instagram' %}
    {{instagram}}
    {% elseif module.data.text|lower == 'vk' or module.data.text|lower == 'vkontakte' %}
    {{vk}}
    {% elseif module.data.text|lower == 'ok' or module.data.text|lower == 'odnoklassniki' %}
    {{ok}}
    {% elseif module.data.text|lower == 'whatsapp' %}
    {{whatsapp}}
    {% elseif module.data.text|lower == 'viber' %}
    {{viber}}
    {% elseif module.data.text|lower == 'telegram' %}
    {{telegram}}
    {% else %}
    {{other}}
    {% endif %}
</a>
{% endfor %}